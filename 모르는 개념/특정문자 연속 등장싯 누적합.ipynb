    "# 어떤 특정 문자가 연속으로 나올 때 누적합\n",
    "# 안나오면 0\n",
    "N = int(input())\n",
    "\n",
    "\n",
    "for i in range(N):\n",
    "    OX = input()\n",
    "    sumscore = 0\n",
    "    score = 0\n",
    "    for j in OX:\n",
    "        if j == \"O\":\n",
    "            score += 1\n",
    "        else:\n",
    "            score = 0\n",
    "        sumscore += score\n",
    "    print(sumscore)\n",
    "    "
