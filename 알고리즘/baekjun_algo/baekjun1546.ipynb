   "source": [
    "# 각 리스트 요소들의 누적합\n",
    "\n",
    "\n",
    "lst2 = []\n",
    "ans = 0\n",
    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "\n",
    "\n",
    "for i in range(N):\n",
    "    B = A[i] / max(A) * 100\n",
    "    lst2.append(B)\n",
    "    ans += lst2[i]\n",
    "print((ans)/N)\n",
    "\n",
    "\n"
   ]
  }
